+    TEST_ASSERT_EQUAL( IOT_HTTPS_INVALID_PARAMETER, returnCode );
+    TEST_ASSERT_NULL( respHandle );
+
+    /* Test a NULL reqHandle parameters. */
+    returnCode = IotHttpsClient_SendAsync( connHandle, NULL, &respHandle, &testRespInfo );
+    TEST_ASSERT_EQUAL( IOT_HTTPS_INVALID_PARAMETER, returnCode );
+    TEST_ASSERT_NULL( respHandle );
